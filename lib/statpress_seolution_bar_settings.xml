<settings>
  <hide_bullets_count>18</hide_bullets_count>
  <vertical_lines>
    <width>1</width>
  </vertical_lines>
  <data_type>csv</data_type>
  <skip_rows>1</skip_rows>
  <preloader_on_reload>1</preloader_on_reload>
  <add_time_stamp>1</add_time_stamp>
  <thousands_separator>.</thousands_separator>
  <digits_after_decimal>0</digits_after_decimal>
  <background>
    <border_alpha>100</border_alpha>
  </background>
  <plot_area>
    <alpha>100</alpha>
    <margins>
      <left>70</left>
      <right>70</right>
      <top>70</top>
      <bottom>50</bottom>
    </margins>
  </plot_area>
  <grid>
    <x>
      <alpha>10</alpha>
      <dashed>1</dashed>
      <dash_length>3</dash_length>
      <approx_count>9</approx_count>
    </x>
    <y_left>
      <alpha>22</alpha>
    </y_left>
    <y_right>
      <color>990066</color>
      <alpha>21</alpha>
      <dashed>1</dashed>
      <dash_length>1</dash_length>
    </y_right>
  </grid>
  <axes>
    <x>
      <width>1</width>
      <color>0D8ECF</color>
    </x>
    <y_left>
      <width>1</width>
      <color>0D8ECF</color>
    </y_left>
  </axes>
  <values>
    <x>
      <color>990000</color>
      <text_size>9</text_size>
      <rotate>90</rotate>
    </x>
  </values>
  <indicator>
    <x_balloon_enabled>0</x_balloon_enabled>
    <color>0D8ECF</color>
    <x_balloon_text_color>FFFFFF</x_balloon_text_color>
    <line_alpha>50</line_alpha>
    <selection_color>0D8ECF</selection_color>
    <selection_alpha>20</selection_alpha>
  </indicator>
  <balloon>
    <alpha>75</alpha>
    <corner_radius>1</corner_radius>
  </balloon>
  <legend>
    <align>center</align>
    <key>
      <size>14</size>
    </key>
    <values>
      <align>left</align>
    </values>
  </legend>
  <zoom_out_button>
    <text_color_hover>FF0F00</text_color_hover>
  </zoom_out_button>
  <help>
    <button>
      <color>FCD202</color>
      <text_color>000000</text_color>
      <text_color_hover>FF0F00</text_color_hover>
    </button>
    <balloon>
      <text><![CDATA[Click on the graph to turn on/off value baloon <br/><br/>Click on legend key to show/hide graph<br/><br/>Mark the area you wish to enlarge]]></text>
      <color>FCD202</color>
      <text_color>000000</text_color>
    </balloon>
  </help>
  <graphs>
    <graph gid="visitors">
      <title>Visitors</title>
      <color>117744</color>
      <color_hover>FF0000</color_hover>
      <line_width>2</line_width>
      <fill_color>000000</fill_color>
      <fill_alpha>50</fill_alpha>
      <bullet>round_outline</bullet>
      <bullet_size>5</bullet_size>
      <bullet_color>006600</bullet_color>
      <bullet_alpha>50</bullet_alpha>
    </graph>
    <graph gid="pageviews">
      <title>Pageviews</title>
      <axis>right</axis>
      <color>3377B6</color>
      <line_width>1</line_width>
      <fill_alpha>10</fill_alpha>
      <bullet>square</bullet>
      <bullet_size>5</bullet_size>
      <bullet_alpha>50</bullet_alpha>
    </graph>
    <graph gid="spiders">
      <title>Spiders</title>
      <color>83B4D8</color>
      <color_hover>FF0000</color_hover>
      <line_width>3</line_width>
      <bullet>round</bullet>
      <bullet_size>4</bullet_size>
      <bullet_color>000066</bullet_color>
      <bullet_alpha>66</bullet_alpha>
    </graph>
    <graph gid="feeds">
      <title>Feeds</title>
      <color>F38F36</color>
      <color_hover>FF0000</color_hover>
      <line_width>1</line_width>
      <bullet>round_outline</bullet>
      <bullet_size>6</bullet_size>
      <bullet_alpha>50</bullet_alpha>
    </graph>
  </graphs>
  <labels>
    <label lid="0">
      <text>Statistics - Timeline Graphs</text>
      <y>25</y>
      <text_size>13</text_size>
      <align>center</align>
    </label>
  </labels>
  <guides>
    <max_min>1</max_min>
  </guides>
</settings>
